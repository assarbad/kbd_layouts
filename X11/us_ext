/////////////////////////////////////////////////////////////////////////////////
//
// Based on my Windows layout (kbdus_xx).
// On Kubuntu copy into folder /usr/share/X11/xkb/symbols/ and add the following
// line to /usr/share/X11/xkb/rules/evdev.lst under layouts:
//
// us_ext      English (US + DE, PL, Nordic)
//

default
partial alphanumeric_keys modifier_keys
xkb_symbols "unicode"
{
	// ~!@#$%^&*()_+ == TLDE, AE01 .. AE12
	// QWERTYUIOP{}| == AD01 .. AD12, BKSL
	// ASDFGHJKL:"   == AC01 .. AC11
	// ZXCVBNM<>?    == AB01 .. AB10
	name[Group1] = "English (US + DE, IS, PL, Nordic)";
	include "us(euro)"
	key <AB01> { [       NoSymbol,       NoSymbol,      zabovedot,       Zabovedot ] }; // [Z] ≈º  ≈ª
	key <AB02> { [       NoSymbol,       NoSymbol,        eogonek,         Eogonek ] }; // [X] ƒô  ƒò
	key <AB03> { [       NoSymbol,       NoSymbol,         cacute,          Cacute ] }; // [C] ƒá  ƒÜ
	key <AB04> { [       NoSymbol,       NoSymbol,          UFFFD,           UFFFD ] }; // [V] ÔøΩ  ÔøΩ
	key <AB05> { [       NoSymbol,       NoSymbol,          U2114,           U2116 ] }; // [B] ‚Ñî  ‚Ññ
	key <AB06> { [       NoSymbol,       NoSymbol,         nacute,          Nacute ] }; // [N] ≈Ñ  ≈É
	key <AB07> { [       NoSymbol,       NoSymbol,       Greek_mu,        Greek_pi ] }; // [M] Œº  œÄ
	key <AB08> { [       NoSymbol,       NoSymbol,         oslash,        Ooblique ] }; // [<] √∏  √ò
	key <AB09> { [       NoSymbol,       NoSymbol,             ae,              AE ] }; // [>] √¶  √Ü
	key <AB10> { [       NoSymbol,       NoSymbol,          aring,           Aring ] }; // [?] √•  √Ö
	key <AC01> { [       NoSymbol,       NoSymbol,        aogonek,         Aogonek ] }; // [A] ƒÖ  ƒÑ
	key <AC02> { [       NoSymbol,       NoSymbol,         sacute,          Sacute ] }; // [S] ≈õ  ≈ö
	key <AC03> { [       NoSymbol,       NoSymbol,            eth,             Eth ] }; // [D] √∞  √ê
	key <AC04> { [       NoSymbol,       NoSymbol,          U017F,         section ] }; // [F] ≈ø  ¬ß
	key <AC05> { [       NoSymbol,       NoSymbol,          U1D79,           UA77D ] }; // [G] ·µπ  ÍùΩ
	key <AC06> { [       NoSymbol,       NoSymbol,         U1F601,          U1F9D0 ] }; // [H] üòÅ üßê
	key <AC07> { [       NoSymbol,       NoSymbol,          U00EF,           U00CF ] }; // [J] √Ø  √è
	key <AC08> { [       NoSymbol,       NoSymbol,         U1F609,          U1F928 ] }; // [K] üòâ ü§®
	key <AC09> { [       NoSymbol,       NoSymbol,        lstroke,         Lstroke ] }; // [L] ≈Ç  ≈Å
	key <AC10> { [       NoSymbol,       NoSymbol,     odiaeresis,      Odiaeresis ] }; // [:] √∂  √ñ
	key <AC11> { [       NoSymbol,       NoSymbol,     adiaeresis,      Adiaeresis ] }; // ["] √§  √Ñ
	key <AD01> { [       NoSymbol,       NoSymbol,             at,        EuroSign ] }; // [Q] @  ‚Ç¨
	key <AD02> { [       NoSymbol,       NoSymbol,          U01BF,           U01F7 ] }; // [W] ∆ø  «∑
	key <AD03> { [       NoSymbol,       NoSymbol,         eacute,          Eacute ] }; // [E] √©  √â
	key <AD04> { [       NoSymbol,       NoSymbol,      copyright,      registered ] }; // [R] ¬©  ¬Æ
	key <AD05> { [       NoSymbol,       NoSymbol,      trademark                  ] }; // [T] ‚Ñ¢
	key <AD06> { [       NoSymbol,       NoSymbol,         yacute,          Yacute ] }; // [Y] √Ω  √ù
	key <AD07> { [       NoSymbol,       NoSymbol,         uacute,          Uacute ] }; // [U] √∫  √ö
	key <AD08> { [       NoSymbol,       NoSymbol,         iacute,          Iacute ] }; // [I] √≠  √ç
	key <AD09> { [       NoSymbol,       NoSymbol,         oacute,          Oacute ] }; // [O] √≥  √ì
	key <AD10> { [       NoSymbol,       NoSymbol,          thorn,           Thorn ] }; // [P] √æ  √û
	key <AD11> { [       NoSymbol,       NoSymbol,     udiaeresis,      Udiaeresis ] }; // [{] √º  √ú
	key <AD12> { [       NoSymbol,       NoSymbol,     dead_grave                  ] }; // D`
	key <AE01> { [       NoSymbol,       NoSymbol,     onequarter,      exclamdown ] }; // [1] ¬º  ¬°
	key <AE02> { [       NoSymbol,       NoSymbol,        onehalf,     twosuperior ] }; // [2] ¬Ω  ¬≤
	key <AE03> { [       NoSymbol,       NoSymbol,  threequarters,   threesuperior ] }; // [3] ¬æ  ¬≥
	key <AE04> { [       NoSymbol,       NoSymbol,         careof,        EuroSign ] }; // [4] ‚ÑÖ  ‚Ç¨
	key <AE05> { [       NoSymbol,       NoSymbol,       NoSymbol,           U2030 ] }; // [5] ‚Ç¨  ‚Ä∞ (‚Ç¨ is inherited by us(euro))
	key <AE06> { [       NoSymbol,       NoSymbol,     dead_caron                  ] }; // [6]
	key <AE07> { [       NoSymbol,       NoSymbol,          U0307                  ] }; // [7]
	key <AE08> { [       NoSymbol,       NoSymbol,     dead_tilde,        infinity ] }; // [8]
	key <AE09> { [       NoSymbol,       NoSymbol,   dead_cedilla                  ] }; // [9]
	key <AE10> { [       NoSymbol,       NoSymbol,dead_circumflex                  ] }; // [0]
	key <AE11> { [       NoSymbol,       NoSymbol,         ssharp,      Greek_beta ] }; // [-] √ü  Œ≤
	key <AE12> { [       NoSymbol,       NoSymbol,     dead_acute                  ] }; // D¬¥
	key <BKSL> { [       NoSymbol,       NoSymbol, dead_diaeresis                  ] }; // D¬®
	key <TLDE> { [       NoSymbol,       NoSymbol,         degree,  dead_abovering ] }; // ¬∞  D¬∞

	key <RWIN> { [       Multi_key                                                 ] };
};
